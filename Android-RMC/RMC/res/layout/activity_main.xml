<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/AbsoluteLayout1"
    android:layout_width="400dp"
    android:layout_height="525dp"
    android:background="@drawable/bg"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:scaleType="centerCrop"
    tools:context=".MainActivity" >
  
    <EditText
        android:id="@+id/amount1"
        android:layout_width="269dp"
        android:layout_height="wrap_content"
        android:layout_x="9dp"
        android:layout_y="50dp"
        android:ems="10" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="94dp"
        android:layout_y="26dp"
        android:text="Enter For Search..." />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="116dp"
        android:layout_y="230dp"
        android:background="@drawable/uparrow_opt"
        android:src="@drawable/uparrow_opt" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="119dp"
        android:layout_y="299dp"
        android:background="@drawable/select"
        android:src="@drawable/select" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="38dp"
        android:layout_y="298dp"
        android:background="@drawable/left"
        android:src="@drawable/left" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="198dp"
        android:layout_y="293dp"
        android:background="@drawable/right"
        android:src="@drawable/right" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="119dp"
        android:layout_y="371dp"
        android:background="@drawable/down"
        android:src="@drawable/down" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="218dp"
        android:layout_y="383dp"
        android:background="@drawable/back"
        android:src="@drawable/back" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="17dp"
        android:layout_y="382dp"
        android:background="@drawable/cross_opt"
        android:src="@drawable/cross_opt" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="114dp"
        android:layout_y="138dp"
        android:background="@drawable/file"
        android:src="@drawable/file" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:onClick="showDialog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="16dp"
        android:layout_y="138dp"
        android:background="@drawable/exit"
        android:src="@drawable/exit" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="215dp"
        android:layout_y="136dp"
        android:background="@drawable/terminal"
        android:src="@drawable/terminal" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="228dp"
        android:layout_y="0dp"
        android:background="@drawable/download"
        android:src="@drawable/download" />

</AbsoluteLayout>